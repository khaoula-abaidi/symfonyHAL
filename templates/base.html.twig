                      style="margin-top: 8px; margin-right: 8px;"
                      action="/user/login" id="form-login" method="post">
                    <input type="hidden" name="forward-controller"
                           value="index" />
                    <input type="hidden" name="forward-action"
                           value="index" />
                    <div class="btn-group">
                        <button class="btn btn-small btn-primary" type="button"
                                onclick="$('#form-login').submit();" accesskey="l">
                            <i class="glyphicon glyphicon-user glyphicon-white"></i>&nbsp;Connexion                                    </button>
                        <button class="btn btn-small btn-primary dropdown-toggle" data-toggle="dropdown"
                                type="button">
                            <span class="caret" style="border-top-color: #fff; border-bottom-color: #fff;"></span>
                        </button>

                        <ul class="dropdown-menu pull-right">
                            <li><a href="#" onclick="$('#form-login').submit();">Connexion</a></li>
                            <li><a id="getorcid" href="https://orcid.org/oauth/authorize?client_id=APP-O6Y5HZD2SFM7ON6Z&response_type=code&scope=/authenticate&redirect_uri=https://hal.archives-ouvertes.fr/user/coext?url=https://hal.archives-ouvertes.fr/">Connexion avec ORCID</a></li>
                            <li class="divider"></li>
                            <li><a href="/user/create">Créer un compte</a></li>
